rules:
 - AddSemiAfterIfForWhileCondition
#mid - AllocaInLoop
 - ArgumentOfSizeofIsExpression
 - ArrayIndexOutOfBound
 - AssignInCondition
 - AssignSameVariableTwiceSuccessively
 - AssignWithUninitVariable
 - AssignZeroValueToPointer
 - BoolIncrement
 - ClassAsStringArgument
 - CmpFuncUseInCondition
 - CompareBoolWithInt
 - ComparePointerWithZeroValue
 - ConstantValueWithZeroPrefix
 - ConstResultAccordToValueRange
 - DereferencingNullPointer
 - DifferentEnumComparn
 - DividingSizeofAPointerByAnotherValue
 - ElseAlignWhichIfProblem
 - EmptyExceptionHandler
 - FormatStringIntoItselfInSprintf
#error - IdenticalSubExprs
 - IllegalThrowWithoutInTryCatchInDestructor
 - InvalidPointToLocalArray
 - InvalidPointToLocalVariable
 - InvalidSizeOfArgumentArrayPoint
 - IteratorsAsArgument
 - LowerPriorityOfConditionalExpression
 - MemFunCallLeadOverOrUnderFlowWithSizeof
 - MissingSemicolonAfterReturn
 - NonSensicalPointerComparn
 - NoNullFunctionPointerComparedWithNull
 - OddFloatPreciseComparision
 - OddTypeCast
 - PartOfConditionIsConstantValue
 - PointerUnaryAndDerefConfuse
 - RepeatedIfCondition
 - SameArgumentForFuncCall
 - SameVariableUseInForLoop
 - StrlenConstantValueInMalloc
#very slow - ThenAndElseBodyEquivalent
 - TwoDimArrayToPointer
#error# - TwoFunctionBodyEquivalent
 - TwoSizeofMultiplied
 - UnsignedValueMinusComapre
 - UseCommaInArrayIndex
 - UseUnSenseCommaInExpression
 - WithoutUseReturnValueOfFunction
 - WrongVariableComparedInForLoop
 - WrongVariableIncreaseInForLoop
#rulePaths:
rule-paths:
